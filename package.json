{
  "name": "sencondTest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "start-pwa": "ng build --configuration production --aot --base-href=/ && http-server -p 8080 -c-1 dist/sencondTest",
    "deploy": "ng build --configuration production --aot && firebase deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.8",
    "@angular/cdk": "^16.1.7",
    "@angular/common": "^16.1.8",
    "@angular/compiler": "^16.1.8",
    "@angular/core": "^16.1.8",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.1.8",
    "@angular/material": "^16.1.7",
    "@angular/platform-browser": "^16.1.8",
    "@angular/platform-browser-dynamic": "^16.1.8",
    "@angular/router": "^16.1.8",
    "@angular/service-worker": "^16.1.8",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@popperjs/core": "^2.11.6",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.4.1",
    "http-server": "^14.1.1",
    "ng2-charts": "^5.0.4",
    "rxjs": "~7.8.0",
    "subsink": "^1.0.2",
    "sweetalert2": "^11.7.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.8",
    "@angular/localize": "^16.1.8",
    "@types/aos": "^3.0.4",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "typescript": "~4.9.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/config.jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.)": "<rootDir>/src/app/$1",
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
